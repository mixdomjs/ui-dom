{
    "name": "ui-dom",
    "version": "2.2.0",
    "description": "Powerful state based DOM rendering library (in JS/TS) - inspired by React, but totally reconceptualized.",
    "keywords": [
        "ui-dom",
        "uidom",
        "ui",
        "dom",
        "render",
        "react",
        "user interface",
        "virtual dom",
        "front-end",
        "library",
        "framework"
    ],
    "homepage": "https://uidomjs.org",
    "author": "Mikael Saur√©n",
    "license": "MIT",
    "private": false,
    "repository": {
      "type": "git",
      "url": "git+https://github.com/ui-dom/ui-dom.git"
    },
    "module": "./dist/ui-dom.module.js",
    "main": "./dist/ui-dom.js",
    "types": "./dist/ui-dom.d.ts",
    "exports": {
        ".": {
            "import": "./dist/ui-dom.module.js",
            "require": "./dist/ui-dom.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "src",
        "CONTRIBUTING.md"
    ],
    "scripts": {
        "tsc": "tsc -p tsconfig.json",
        "build": "tsc && rollup -c rollup.config.js"
    },
    "devDependencies": {
        "rollup": "^2.77.2",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-terser": "^7.0.2",
        "typescript": "^4.7.4"
    },
    "dependencies": {}
}
